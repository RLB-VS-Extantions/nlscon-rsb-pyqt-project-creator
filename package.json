{
  "name": "nlscon-rsb-pyqt-project-creator",
  "displayName": "NLScon-RSB PyQT Project Creator",
  "publisher": "RealLab",
  "version": "1.0.0",
  "description": "Создавайте PyQt проекты для PLC NLScon-RSB с одной кнопки",
  "categories": ["Other"],
  "keywords": ["reallab", "pyqt", "nlscon", "plc", "embedded"],
    "repository": {
    "type": "git",
    "url": "https://github.com/RLB-VS-Extantions/nlscon-rsb-pyqt-project-creator"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "extensionKind": ["workspace"],
  
  "activationEvents": [
    "onCommand:nlscon-rsb-pyqt-project-creator.createProject"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nlscon-rsb-pyqt-project-creator.createProject",
        "title": "NLScon-RSB: Create Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "mocha": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "license": "MIT"
}